#include <iostream>
#include <algorithm>
using namespace std;

struct Job {
    char id;
    int deadline;
    int profit;
};

// Compare jobs based on profit (descending)
bool compare(Job a, Job b) {
    return a.profit > b.profit;
}

void jobSchedule(Job arr[], int n) {
    // Sort jobs by profit in decreasing order
    sort(arr, arr + n, compare);

    int result[n];
    bool slot[n];

    // Initialize all slots to free
    for (int i = 0; i < n; i++) {
        slot[i] = false;
    }

    // For each job
    for (int i = 0; i < n; i++) {
        // Find a free slot from its deadline to the start
        for (int j = min(n, arr[i].deadline) - 1; j >= 0; j--) {
            if (slot[j] == false) {
                result[j] = i;
                slot[j] = true;
                break;
            }
        }
    }

    cout << "\nMaximum Profit Job Sequence: ";
    for (int i = 0; i < n; i++) {
        if (slot[i]) {
            cout << arr[result[i]].id << " ";
        }
    }
    cout << endl;
}

int main() {
    int n;
    cout << "Enter number of jobs: ";
    cin >> n;

    Job arr[n];

    cout << "Enter Job Details (ID Deadline Profit):\n";
    for (int i = 0; i < n; i++) {
        cin >> arr[i].id >> arr[i].deadline >> arr[i].profit;
    }

    jobSchedule(arr, n);

    return 0;
}
